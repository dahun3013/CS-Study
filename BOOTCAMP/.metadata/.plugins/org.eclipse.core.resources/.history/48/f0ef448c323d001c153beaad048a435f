package day4;

public class ForEx {
	public static void main(String[] args) {
		boolean[] ba1;
		ba1 = new boolean[] {true,true,false};
		String[] sa1;
		sa1 = new String[] {"È«±æµ¿","±èÀ¯½Å"};
		float[] fa1;
		fa1=new float[] {1.1f, 2.2f, 3.3f};
		
		for(int i=0; i<sa1.length;i++) {
			System.out.println(sa1[i]);
		}
	}

}

