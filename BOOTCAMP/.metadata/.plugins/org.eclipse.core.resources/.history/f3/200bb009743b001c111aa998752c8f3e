package day1;

public class EX2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("byte형의 크기 \t" + Byte.SIZE);
		System.out.println("short형의 크기 \t" + Short.SIZE);
		System.out.println("int형의 크기 \t" + Integer.SIZE);
		System.out.println("long형의 크기 \t" + Long.SIZE);
		System.out.println("float형의 크기 \t" + Float.SIZE);
		System.out.println("double형의 크기 \t" + Double.SIZE);
		System.out.println("char형의 크기 \t" + Character.SIZE);
		
	}

}
